{% extends "::base.html.twig" %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{asset('bundles/app/css/common.css')}}" />
    <link rel="stylesheet" href="{{asset('bundles/app/css/sgk_basic.css')}}" />
    <style type='text/css'>
        @font-face{
            font-family: "Nanum Gothic";
            src: url('{{asset("bundles/app/css/fonts/NanumGothic.eot")}}');
            src: url('{{asset("bundles/app/css/fonts/NanumGothic.eot?#iefix")}}') format('embedded-opentype'),
            url('{{asset("bundles/app/css/fonts/NanumGothic.woff")}}') format('woff'),
            url('{{asset("bundles/app/css/fonts/NanumGothic.ttf")}}') format('truetype');
            url('{{asset("bundles/app/css/fonts/NanumGothic.svg#NanumGothic")}}') format('svg')
            src: local(��), url('{{asset("bundles/app/css/fonts/NanumGothic.woff")}}') format('woff');
    }

    </style>
{{ parent() }}
{% endblock %}
{% block body %}    
    <div id="dark_back"></div>
    {% block popup %}{% endblock%}

    <div id="header_back"  style="background-image: url({{asset('bundles/app/image/sgk_header.png')}});">
    {% block header %}{% endblock%}
    </div> 
    <div id="background1" style="background-image: url({{asset('bundles/app/image/sgk_paperBack.jpg')}});">
        <div id="background2" style="background-image: url({{asset('bundles/app/image/sgk_Background2.png')}});">
        </div>
                <div id="contents_box">
                    <div id="top_contents">
                    {% block event %}
                    {% endblock %}
                    {% block minsession %}
                    {% endblock %}
                    </div>
                    <div id="content_body">    
                    {% block contents %} 
                    {% endblock %}                       
                    </div>
                    <div id="sidebar_box">
                    {% block side %}{% endblock%}
                    </div>
{#                    <div style="clear:both"></div>#}
                </div>
            </div>
                <div id="footer">
                {% block footer %}
                   {% include 'AppBundle:Sgk:footer.html.twig'%}
                {% endblock %}
                </div>
                
{% endblock %}
 
