{% extends "AppBundle::sgk_layout.html.twig" %}
{% block stylesheets %}
        {{ parent() }}
<link rel="stylesheet" href="{{asset('bundles/app/css/sgk_world.css')}}" />
{% endblock %}
{% block header %}{% include 'AppBundle:Sgk:header.html.twig'%}{% endblock%}
{% block javascripts %}
    {{ parent() }}
    <script src="{{asset("bundles/app/js/sgk_world_javascript.js")}}" ></script>
{% endblock %}
{% block popup %}
    <div id="fox" >
        <div id="fox01" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)" ></div>
        <div id="fox02" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)"></div>
        <div id="fox03" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)"></div>
        <div id="fox04" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)"></div>
        <div id="fox00" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)"></div>
        <div id="fox05" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)"></div>
        <div id="fox06" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)"></div>
        <div id="fox07" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)"></div>
        <div id="fox08" class="A_fox" ondrop="drop(event);"  ondragover="allowDrop(event)"></div>
    </div>
    <div id="popup_page">
        <div>
            <div id="popup_Q1" class="popup_change">
                <div id="video_title1" class="video_title"></div>
                <div id="video_1" class="world_video" title="스타트 가이드">
                    <iframe width="640" height="360" src="https://www.youtube.com/embed/-o6_ueO6AJI?rel=1" frameborder="0" allowfullscreen></iframe>
                </div>
            </div>
            <div id="popup_Q2" class="popup_change">
